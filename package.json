{
  "name": "remix-dashboard",
  "version": "1.0.0",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve ./build/server/index.js",
    "typecheck": "tsc",
    "test": "NODE_ENV=test playwright test",
    "test:ui": "NODE_ENV=test playwright test --ui",
    "db:drop": "rm -rf db.sqlite*",
    "db:check": "drizzle-kit check:sqlite",
    "db:migrate": "drizzle-kit generate:sqlite",
    "db:push": "drizzle-kit push:sqlite",
    "db:seed": "tsx ./app/db/seed.server.ts",
    "db:studio": "drizzle-kit studio --port 3000 --verbose"
  },
  "dependencies": {
    "@faker-js/faker": "^8.3.1",
    "@heroicons/react": "^2.0.18",
    "@remix-run/css-bundle": "^2.8.1",
    "@remix-run/node": "^2.8.1",
    "@remix-run/react": "^2.8.1",
    "@remix-run/serve": "^2.8.1",
    "@remix-validated-form/with-zod": "^2.0.7",
    "better-sqlite3": "^9.1.1",
    "clsx": "^2.0.0",
    "daisyui": "4.0.7",
    "drizzle-orm": "^0.29.1",
    "isbot": "^3.8.0",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3",
    "playwright": "^1.40.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-toast": "^1.1.0",
    "remix-utils": "^7.1.0",
    "remix-validated-form": "^5.1.5",
    "sonner": "^1.2.2",
    "use-debounce": "^10.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@remix-run/dev": "^2.8.1",
    "@remix-run/eslint-config": "^2.8.1",
    "@types/better-sqlite3": "^7.6.9",
    "@types/node": "^20.10.2",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "drizzle-kit": "^0.20.1",
    "eslint": "^8.38.0",
    "eslint-plugin-tailwindcss": "^3.15.1",
    "tailwindcss": "^3.3.5",
    "tsx": "^4.7.1",
    "typescript": "^5.1.6",
    "vite": "^5.2.6",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
