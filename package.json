{
  "name": "remix-dashboard",
  "version": "1.0.0",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve ./build/server/index.js",
    "typecheck": "tsc",
    "test": "NODE_ENV=test playwright test --retries=1",
    "test:ui": "NODE_ENV=test DEBUG=pw:webserver playwright test --ui",
    "db:drop": "rm -rf ./sqlite/*",
    "db:check": "drizzle-kit check:sqlite",
    "db:migrate": "drizzle-kit generate:sqlite",
    "db:push": "drizzle-kit push:sqlite",
    "db:seed": "tsx ./app/db/seed.server.ts",
    "db:studio": "drizzle-kit studio --port 3000 --verbose"
  },
  "dependencies": {
    "@heroicons/react": "^2.1.3",
    "@remix-run/node": "^2.8.1",
    "@remix-run/react": "^2.8.1",
    "@remix-run/serve": "^2.8.1",
    "@remix-validated-form/with-zod": "^2.0.7",
    "better-sqlite3": "^9.4.5",
    "clsx": "^2.1.0",
    "daisyui": "4.9.0",
    "drizzle-orm": "^0.30.7",
    "isbot": "^5.1.4",
    "pino": "^8.19.0",
    "pino-pretty": "^11.0.0",
    "playwright": "^1.42.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-toast": "^1.2.0",
    "remix-utils": "^7.5.0",
    "remix-validated-form": "^5.1.5",
    "sonner": "^1.4.41",
    "use-debounce": "^10.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@playwright/test": "^1.42.1",
    "@remix-run/dev": "^2.8.1",
    "@remix-run/eslint-config": "^2.8.1",
    "@types/better-sqlite3": "^7.6.9",
    "@types/node": "^20.12.4",
    "@types/react": "^18.2.74",
    "@types/react-dom": "^18.2.24",
    "autoprefixer": "^10.4.19",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.20.14",
    "eslint": "^8.57.0",
    "eslint-plugin-tailwindcss": "^3.15.1",
    "tailwindcss": "^3.4.3",
    "tsx": "^4.7.2",
    "typescript": "^5.4.3",
    "vite": "^5.2.8",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "trustedDependencies": [
    "better-sqlite3",
    "es5-ext",
    "esbuild"
  ]
}
