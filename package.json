{
  "name": "remix-dashboard",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "vite build && vite build --ssr",
    "dev": "vite dev",
    "start": "remix-serve ./build/index.js",
    "typecheck": "tsc",
    "db:migrations": "drizzle-kit generate:pg --schema ./app/db/schema.ts --out ./app/db/migrations -",
    "db:push": "drizzle-kit push:pg",
    "db:seed": "tsx ./app/db/seed.server.ts"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "@remix-run/css-bundle": "^2.2.0",
    "@remix-run/node": "^2.2.0",
    "@remix-run/react": "^2.2.0",
    "@remix-run/serve": "^2.2.0",
    "@remix-validated-form/with-zod": "^2.0.7",
    "clsx": "^2.0.0",
    "daisyui": "4.0.7",
    "drizzle-orm": "^0.29.0",
    "isbot": "^3.6.8",
    "postgres": "^3.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-auth": "^3.6.0",
    "remix-auth-form": "^1.4.0",
    "remix-hook-form": "^3.0.4",
    "remix-validated-form": "^5.1.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@remix-run/dev": "^2.2.0",
    "@remix-run/eslint-config": "^2.2.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "dotfiles": "^0.8.0",
    "drizzle-kit": "^0.20.1",
    "eslint": "^8.38.0",
    "tailwindcss": "^3.3.5",
    "tsx": "^4.1.4",
    "typescript": "^5.1.6",
    "vite": "^4.5.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
